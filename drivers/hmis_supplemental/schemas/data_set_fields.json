
{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "key": {
        "type": "string",
        "pattern": "^[a-z_$][a-z0-9_$]*$"
      },
      "label": {
        "type": "label"
      },
      "type": {
        "type": "string",
        "enum": ["string", "int", "float", "boolean", "date"]
      },
      "multiValued": {
        "type": "boolean"
      }
    },
    "required": ["key", "type", "label"],
    "additionalProperties": false
  },
  "minItems": 1
}
