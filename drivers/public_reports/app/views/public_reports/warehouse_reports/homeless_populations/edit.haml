- title = "Edit #{@report.instance_title}"
- content_for :title, title
= render 'breadcrumbs'
%h1.mb-5= title

= render 'public_reports/publish'

- @report.populations.each_key do |population|
  %p Once published, the following will be available at the url: #{@report.generate_publish_url_for(population)}
  .well
    - src = polymorphic_path([population, :public_reports, :warehouse_reports, :homeless_population], pp: :disabled)
    %iframe{src: src, width: '100%', height: 400, frameBorder: 0}
