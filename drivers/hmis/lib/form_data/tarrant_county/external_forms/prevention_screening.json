{
  "name": "Tarrant County Prevention Screening Form",
  "item": [
    {
      "type": "DISPLAY",
      "text": "Do not leave any portion of the assessment blank",
      "link_id": "required_fields_explanation"
    },
    {
      "type": "GROUP",
      "text": "About You",
      "link_id": "about_you",
      "item": [
        {
          "link_id": "first_name",
          "type": "STRING",
          "required": true,
          "text": "First Name",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_first_name"
          }
        },
        {
          "link_id": "last_name",
          "type": "STRING",
          "required": true,
          "text": "Last Name",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_last_name"
          }
        },
        {
          "link_id": "address_apartment_number",
          "type": "STRING",
          "required": true,
          "text": "Address & Apartment Number",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_address_apartment_number"
          }
        },
        {
          "link_id": "city",
          "type": "STRING",
          "required": true,
          "text": "City",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_city"
          }
        },
        {
          "link_id": "resident_of_arlington_fort_worth_or_grand_prairie",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Do you live outside of the following cities: Arlington, Fort Worth, or Grand Prairie?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_resident_of_arlington_fort_worth_or_grand_prairie"
          }
        },
        {
          "link_id": "client_email",
          "type": "STRING",
          "required": true,
          "text": "Your Email",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_client_email"
          }
        },
        {
          "link_id": "client_phone_number",
          "type": "STRING",
          "required": true,
          "text": "Your Phone Number",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_client_phone_number"
          }
        },
        {
          "link_id": "dob",
          "type": "DATE",
          "required": true,
          "text": "Your Date of Birth",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_dob"
          }
        }
      ]
    },
    {
      "type": "GROUP",
      "text": "Overview",
      "link_id": "overview",
      "item": [
        {
          "type": "DISPLAY",
          "link_id": "assessment_evaluation_notice",
          "text": "<p>Please note: Assessments will be evaluated for eligibility and completeness. Tenants will be notified or asked for additional requested items by email.</p><p>Tenants MUST NOT have received rental assistance from any other sources for which tenant is requesting at this time.</p>",
          "readonly_text": ""
        },
        {
          "link_id": "military_veteran_in_home",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Is there a military veteran in the home?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_military_veteran_in_home"
          }
        },
        {
          "type": "DISPLAY",
          "text": "If there is a military veteran in the home, please call Community Actions Partners first at 817-392-5780.",
          "link_id": "military_veteran_notice",
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "tchc_prevention_screening_military_veteran_in_home",
              "operator": "EQUAL",
              "answer_code": "yes"
            }
          ]
        },
        {
          "link_id": "received_rental_assistance_in_last_three_years",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Have you received rental assistance within the past three (3) years?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_received_rental_assistance_in_last_three_years"
          }
        },
        {
          "link_id": "lives_in_unit_with_assisted_by_federal_funding",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Do you live in a unit assisted by another Federal Funded housing, such as Section 8?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_lives_in_unit_with_assisted_by_federal_funding"
          }
        },
        {
          "link_id": "explanation_of_non_payment_of_rent",
          "type": "TEXT",
          "required": true,
          "text": "Please explain current Household \"Crisis\" that has resulted in the non-Payment of rent for your household this month.",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_explanation_of_non_payment_of_rent"
          }
        },
        {
          "link_id": "has_been_literally_homeless",
          "type": "CHOICE",
          "component": "RADIO_BUTTONS",
          "required": true,
          "text": "Have you ever been literally homeless (Stayed in an Emergency Shelter or Outside or a place not meant for habitation)?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_has_been_literally_homeless"
          }
        }
      ]
    },
    {
      "type": "GROUP",
      "text": "Rental History",
      "link_id": "rental_history",
      "item": [
        {
          "link_id": "how_many_bedrooms_is_your_unit",
          "type": "CHOICE",
          "required": true,
          "component": "DROPDOWN",
          "text": "How many bedrooms is your unit?",
          "pick_list_options": [
            {
              "code": "studio",
              "label": "Studio"
            },
            {
              "code": "one_bedroom",
              "label": "1 Bedroom"
            },
            {
              "code": "two_bedrooms",
              "label": "2 Bedrooms"
            },
            {
              "code": "three_bedrooms",
              "label": "3 Bedrooms"
            },
            {
              "code": "four_bedrooms",
              "label": "4 Bedrooms"
            },
            {
              "code": "five_plus_bedrooms",
              "label": "5+ Bedrooms"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_how_many_bedrooms_is_your_unit"
          }
        },
        {
          "link_id": "have_you_received_a_3_day_notice_to_vacate",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Have you received a \"3 Day Notice to Vacate\" from your property management?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_have_you_received_a_3_day_notice_to_vacate"
          }
        },
        {
          "link_id": "have_you_been_served_with_eviction_court_date",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Have you been served by the Tarrant County Constable Office with a formal eviction court date?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_have_you_been_served_with_eviction_court_date"
          }
        },
        {
          "link_id": "how_many_people_on_lease_are_over_18yo",
          "type": "STRING",
          "text": "How many people on the lease are over 18 years old?",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_how_many_people_on_lease_are_over_18yo"
          }
        },
        {
          "link_id": "monthly_rent_amount",
          "type": "STRING",
          "text": "How much is your base rent? (This does NOT include utilities or other incidentals.)",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_monthly_rent_amount"
          }
        },
        {
          "link_id": "landlord_name",
          "type": "STRING",
          "text": "Landlord Name",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_landlord_name"
          }
        },
        {
          "link_id": "landlord_phone_number",
          "type": "STRING",
          "text": "Landlord Phone Number",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_landlord_phone_number"
          }
        },
        {
          "link_id": "landlord_email",
          "type": "STRING",
          "text": "Landlord Email",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_landlord_email"
          }
        }
      ]
    },
    {
      "type": "GROUP",
      "text": "Income Details",
      "link_id": "income details",
      "item": [
        {
          "link_id": "landlord_has_relationship_with_anyone_in_household",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Does the landlord have a relationship to anyone in the household?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_landlord_has_relationship_with_anyone_in_household"
          }
        },
        {
          "link_id": "has_anyone_assisted_with_rent_in_past_12_months",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Has any person or program assisted in paying rent for your household in past 12 months?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_has_anyone_assisted_with_rent_in_past_12_months"
          }
        },
        {
          "link_id": "household_member_gross_income_per_pay_period_or_monthly",
          "type": "STRING",
          "text": "What is the GROSS Income per pay period or Monthly Income for household members over age 18?",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_household_member_gross_income_per_pay_period_or_monthly"
          }
        },
        {
          "text": "What forms of income were received within the past 30 days? (All income sources will be counted. Proof of employment termination may be needed.)",
          "link_id": "forms_of_income",
          "type": "GROUP",
          "component": "INPUT_GROUP",
          "item": [
            {
              "link_id": "in_past_30_days_had_income_from_ssi",
              "type": "BOOLEAN",
              "component": "CHECKBOX",
              "text": "SSI",
              "mapping": {
                "custom_field_key": "tchc_prevention_screening_in_past_30_days_had_income_from_ssi"
              }
            },
            {
              "link_id": "in_past_30_days_had_income_from_ssdi",
              "type": "BOOLEAN",
              "component": "CHECKBOX",
              "text": "SSDI",
              "mapping": {
                "custom_field_key": "tchc_prevention_screening_in_past_30_days_had_income_from_ssdi"
              }
            },
            {
              "link_id": "in_past_30_days_had_income_from_veterans_benefits",
              "type": "BOOLEAN",
              "component": "CHECKBOX",
              "text": "Veteran's Benefits",
              "mapping": {
                "custom_field_key": "tchc_prevention_screening_in_past_30_days_had_income_from_veterans_benefits"
              }
            },
            {
              "link_id": "in_past_30_days_had_income_from_food_stamps",
              "type": "BOOLEAN",
              "component": "CHECKBOX",
              "text": "Food Stamps",
              "mapping": {
                "custom_field_key": "tchc_prevention_screening_in_past_30_days_had_income_from_food_stamps"
              }
            },
            {
              "link_id": "in_past_30_days_had_income_from_child_support",
              "type": "BOOLEAN",
              "component": "CHECKBOX",
              "text": "Child Support",
              "mapping": {
                "custom_field_key": "tchc_prevention_screening_in_past_30_days_had_income_from_child_support"
              }
            },
            {
              "link_id": "in_past_30_days_had_income_from_unemployment",
              "type": "BOOLEAN",
              "component": "CHECKBOX",
              "text": "Unemployment",
              "mapping": {
                "custom_field_key": "tchc_prevention_screening_in_past_30_days_had_income_from_unemployment"
              }
            }
          ]
        },
        {
          "link_id": "lost_employment_or_income_in_last_30_days",
          "type": "CHOICE",
          "component": "RADIO_BUTTONS",
          "required": true,
          "text": "Did you lose employment or have lower income within the past 30 days?",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_lost_employment_or_income_in_last_30_days"
          }
        },
        {
          "link_id": "household_member_notified_of_income_or_state_benefits",
          "type": "CHOICE",
          "required": true,
          "component": "RADIO_BUTTONS",
          "text": "Has anyone in your household received written notice that they will start receiving income and or state benefits? (Ex: Job offer letter, unemployment benefits, SNAP benefits, child support, etc.)",
          "pick_list_options": [
            {
              "code": "yes",
              "label": "Yes"
            },
            {
              "code": "no",
              "label": "No"
            }
          ],
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_household_member_notified_of_income_or_state_benefits"
          }
        }
      ]
    },
    {
      "type": "GROUP",
      "text": "Applicant Certification",
      "link_id": "applicant_certification",
      "item": [
        {
          "link_id": "applicant_certification_authorization",
          "type": "BOOLEAN",
          "required": true,
          "component": "CHECKBOX",
          "text": "I/we understand that the above information is being collected to determine if I/we are eligible to receive rental assistance. I/we authorize the program administrator to verify all information provided on this assessment.",
          "mapping": {
            "custom_field_key": "tchc_prevention_screening_applicant_certification_authorization"
          }
        }
      ]
    }
  ]
}
