= f.input :active
= f.input :s3_access_key_id
= f.input :s3_secret_access_key, as: :password, input_html: { value: @config.s3_secret_access_key }
= f.input :s3_region, collection: GrdaWarehouse::RecurringHmisExport.available_s3_regions, as: :select_two
= f.input :s3_bucket_name
= f.input :s3_path
= f.input :zip_file_password, as: :string, input_html: { value: @config.zip_file_password }
= f.input :file_count, label: 'How many files should we attempt to import daily?', hint: 'This will not re-import a file with the same name, but can be used if the datasource is split across multiple files.', collection: (1..10).to_a, as: :select_two
