= f.error_notification

.row
  .col-sm-6
    .form-inputs
      = f.input :fetch
      = f.input :active
      = f.input :confidential
      = f.input :exclude_from_window
      = f.input :health, hint: 'Enabling this will expose the touch point on the Healthcare side of the warehouse'
      = f.input :hud_assessment, label: 'HUD Entry/Exit/Update'
      = f.input :vispdat, label: 'VI-SPDAT'
      = f.input :pathways
      = f.input :triage_assessment
      = f.input :rrh_assessment, label: 'RRH Assessment'
      = f.input :ssm, label: 'Does this represent an SSM?'
      = f.input :health_case_note, label: 'Is this a Health care case note?'
      = f.input :health_has_qualifying_activities, label: 'Contains Qualifying Activities (Health)'
      = f.input :covid_19_impact_assessment, label: 'COVID-19 Impact Assessment'
      = f.input :with_location_data, label: 'Does this contain latitude and longitude data?'

    .form-actions
      = f.button :submit, "Submit", class: 'btn btn-primary'
