- if @blank_files.present?
  %section.o-section-card
    %header.o-section-card__header
      %h3= file_list_title
    .c-card.c-card--flush.c-card--block
      %table.table.table-striped
        %thead
          %tr
            %th Document
        %tbody
          - @blank_files.each do |file|
            - if file[:url].present?
              %tr
                %td
                  = link_to file[:url] do
                    %i.mr-2.pull-right.icon-download2
                    = Translation.translate(file[:title])
