= render partial: 'breadcrumbs'
.row
  .col-sm-12
    %h1 Service History Generation Logs
.row.table-utilities
  .col-sm-12
    .table-responsive
      %table.table.table-condensed.table-hover
        %thead
          %tr
            %th Started
            %th Execution time
            %th Clients processed
            %th Clients updated
            %th Clients patched
          %tbody
            - @service.each do |d|
              %tr
                %td
                  = d.started_at
                %td
                  = d.import_time
                %td
                  = d.clients_processed
                  of
                  = d.total_clients
                %td
                  = d.clients_updated
                %td
                  = d.clients_patched

%p= paginate @service
