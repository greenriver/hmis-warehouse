- title = "New Access Control"
- content_for :title, title

= render 'breadcrumbs'

%h1= content_for :title
.well
  = simple_form_for @access_control, as: :access_control, url: admin_access_controls_path do |f|
    = f.error_notification
    .form-inputs
      = render 'form', f: f

    .form_actions.mt-4
      = f.button :submit, value: 'Create Access Control'
