= render '/warehouse_reports/breadcrumbs', tab: 'health-bh-cp-claims-payments'
- title = "Claims Processing"
- content_for :title, title
%h1= content_for :title
- case @state
- when :precalculating
  = render 'running'
- when :precalculated
  = render 'review'
- when :running
  = render 'running'
- when :unsubmitted
  = render 'download'
- when :initial
  = render 'initial'
