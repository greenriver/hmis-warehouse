- title = 'Incorrect Move-in Dates'
- content_for :title, title
%h1= title
= render '/warehouse_reports/breadcrumbs', tab: 'data-quality'
= render 'filter'
- if @enrollments.exists?
  .d-flex
    .ml-auto.mb-4
      = link_to({format: :xlsx}.merge(link_params),{class: 'btn btn-secondary ml-auto'}) do
        %i.icon-download2
        Download
  .d-flex
    %p= page_entries_info @enrollments, entry_name: _('enrollment')
    %p.ml-auto= paginate @enrollments
  .table-responsive
    %table.table.table-sm.table-hover
      %thead
        %tr
          %th Client
          %th Project
          %th Entry Date
          %th Move-in Date
          %th Exit Date
          %th Destination
      %tbody
        - @enrollments.each do |en|
          - client = en.client.destination_client
          - project = en.project
          %tr
            %td= link_to client.name, client_path(client)
            %td= project.name
            %td= en.EntryDate
            %td= en.MoveInDate
            %td= en.exit&.ExitDate
            %td= HUD.destination(en.exit&.Destination)
  .d-flex
    %p.ml-auto= paginate @enrollments