.warehouse-reports__chronic.table-responsive
  %table.table.table-striped
    %thead
      %tr
        %th Warehouse ID
        %th Last Name
        %th First Name
        %th SSN
        %th DOB
        %th Veteran Status
    %tbody
      - @clients.each do |client|
        %tr
          %td= client[:id]
          %td= client[:FirstName]
          %td= client[:LastName]
          %td= ssn(client[:SSN])
          %td= client[:DOB]
          %td= HudUtility2024.no_yes_missing(client[:VeteranStatus])
