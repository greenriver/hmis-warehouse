- relative_total ||= values.sum
- colors ||= ['#fca736', '#ffe09b']
- labels ||= ['asynchronous', 'concurrent']
.progress(style='background: #fff; border: 1px solid #ccc;')
  - values.each_with_index do |value, i|
    - title = "#{value} #{labels[i]}ly shared clients with #{filters.coc2.cocnum}"
    .progress-bar{style: "width: #{(value.to_f/relative_total.to_f)*100}%; background: #{colors[i]}", data: {toggle:"tooltip"}, title: title}
