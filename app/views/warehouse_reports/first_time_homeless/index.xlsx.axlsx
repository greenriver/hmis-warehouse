wb = xlsx_package.workbook
wb.add_worksheet(name: "First Time Homeless #{GrdaWarehouse::WarehouseReports::Dashboard::Base.available_sub_populations.invert[@sub_population]}"[0,30]) do |sheet|
  title = sheet.styles.add_style(sz: 12, b: true, alignment: {horizontal: :center})
  sheet.add_row(['Warehouse Client ID','First Name', 'Last Name', 'Age', 'Date of First Service', 'Project Type', 'Project', 'Organization', 'Data Sources'], :style => title)
  @clients.each do |client|
    sheet.add_row [client.id, client.FirstName, client.LastName, client.age(), client.first_service_history.date, ::HUD.project_type(client.first_service_history.project_type), client.first_service_history.project_name, client.first_service_history.organization.try(:OrganizationName), client.source_clients.map{|m| m.data_source.short_name}.uniq.join(', ') ] 
  end
end