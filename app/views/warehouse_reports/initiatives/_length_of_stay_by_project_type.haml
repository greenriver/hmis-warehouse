- data = @data.send(section)
- if data.present?
  %h4= title
  = render 'bar_from_table', section: section
  %table.table.table-striped.jToggle{id: "#{section}_table"}
    %thead
      %tr
        %th Project Type
        - GrdaWarehouse::Hud::Enrollment.lengths_of_stay.each do |l_key, _|
          %th= l_key.to_s.humanize
    %tbody
      - @data.involved_project_types.each do |p_type|
        %tr
          %th= p_type
          - GrdaWarehouse::Hud::Enrollment.lengths_of_stay.each do |l_key, _|
            %td
              - key = "#{p_type}__#{l_key}"
              - count = data[key] || 0
              = link_to_if(count > 0 && can_administer_assigned_reports?, count, warehouse_report_support_index_path(@report.id, key: key, section: section, title: "#{section.to_s.titleize} for: #{p_type}â€”#{l_key.to_s.humanize}"), data: { loads_in_pjax_modal: true })