- data = @data.send(section)
- if data.present?
  %h4= title

  %table.table.table-striped.jToggle{id: "#{section}_table"}
    %thead
      %tr
        %th Project Type
        - range.to_a.each do |date|
          %th= date
    %tbody
      - @data.involved_project_types.each do |p_type|
        %tr
          %th= p_type
          - range.to_a.each do |date|
            %td
              - key = "#{p_type}__#{date}"
              - count = data[key] || 0
              = link_to_if(count > 0 && can_administer_assigned_reports?, count, warehouse_report_support_index_path(@report.id, key: key, section: section), data: { loads_in_pjax_modal: true })