- data = @data.send(section)
- if data.present?
  %h4= title
  = render 'bar_from_table', section: section
  %table.table.table-striped.jToggle{id: "#{section}_table"}
    %thead
      %tr
        %th Project
        - ::HUD.races.each do |column, value|
          %th= value
    %tbody
      - @data.involved_projects.sort_by(&:last).each do |(p_id, p_name)|
        %tr
          %th= p_name
          - ::HUD.races.each do |id, value|
            %td
              - key = "#{p_id}__#{value.parameterize.underscore}"
              - count = data[key] || 0
              = link_to_if(count > 0 && can_administer_assigned_reports?, count, warehouse_report_support_index_path(@report.id, key: key, section: section, title: "#{section.to_s.titleize} for: #{p_name}â€”#{value}"), data: { loads_in_pjax_modal: true })