- data = @data.send(section)
- if data.present?
  %h4= title

  %table.table.table-striped.jToggle{id: "#{section}_table"}
    %thead
      %tr
        %th Project
        - range.to_a.each do |date|
          %th= date
    %tbody
      - @data.involved_projects.sort_by(&:last).each do |(p_id, p_name)|
        %tr
          %th= p_name
          - range.to_a.each do |date|
            %td
              - key = "#{p_id}__#{date}"
              - count = data[key] || 0
              = link_to_if(count > 0 && can_administer_assigned_reports?, count, warehouse_report_support_index_path(@report.id, key: key, section: section, title: "#{section.to_s.titleize} for: #{p_name}â€”#{date}"), data: { loads_in_pjax_modal: true })